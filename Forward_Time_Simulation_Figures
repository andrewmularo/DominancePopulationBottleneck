#### Individual simulation plots 


f2ploth0<- ggplot(h0f2, aes(x=generation, y=PI, group = dominance)) +
  labs(x="Generation", y="pi", title = "NF=2")+
  geom_point(aes(color="#990000"))+
  theme_classic()+
  coord_cartesian(ylim = c(0.0,0.51))+
  theme(axis.text.x = element_blank(),
        axis.title.x = element_blank(),
        axis.text.y = element_text(face = "bold", color = "black", size = 15),
        axis.title.y = element_text(face = "bold", color = "black", size = 20),
        title = element_text(size = 25, face = "bold"),
        legend.position = "none")

f20ploth0<- ggplot(h0f20, aes(x=generation, y=PI, group = dominance)) +
  labs(x="Generation", y="pi", title = "NF=20")+
  geom_point(aes(color="#990000"))+
  theme_classic()+
  coord_cartesian(ylim = c(0.0,0.51))+
  theme(axis.text.x = element_blank(),
        axis.title.x = element_blank(),
        axis.text.y = element_text(face = "bold", color = "black", size = 15),
        axis.title.y = element_blank(),
        title = element_text(size = 25, face = "bold"),
        legend.position = "none")

f50ploth0<- ggplot(h0f50, aes(x=generation, y=PI, group = dominance)) +
  labs(x="Generation", y="pi", title = "NF=50")+
  geom_point(aes(color="#990000"))+
  theme_classic()+
  coord_cartesian(ylim = c(0.0,0.51))+
  theme(axis.text.x = element_blank(),
        axis.title.x = element_blank(),
        axis.text.y = element_text(face = "bold", color = "black", size = 15),
        axis.title.y = element_text(face = "bold", color = "black", size = 20),
        title = element_text(size = 25, face = "bold"),
        legend.position = "none")

f100ploth0<- ggplot(h0f100, aes(x=generation, y=PI, group = dominance)) +
  labs(x="Generation", y="pi", title = "NF=100")+
  geom_point(aes(color="#990000"))+
  theme_classic()+
  coord_cartesian(ylim = c(0.0,0.51))+
  theme(axis.text.x = element_blank(),
